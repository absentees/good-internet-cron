{
    "name": "good-internet-cron",
    "alias": [
        "good-internet-cron.now.sh"
    ],
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } },
        { "src": "screenshotDesignerNews.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" }  },
        { "src": "screenshotThis.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" }  },
        { "src": "good-lib.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" }  }
    ],
    "routes": [
        { "src": "/", "dest": "/index.js"},
        { "src": "/screenshotDesignerNews", "dest": "/screenshotDesignerNews.js"},
        { "src": "/(.+)", "dest": "/screenshotThis.js" }
    ],
    "env": {
        "NETLIFY_DEPLOY_HOOK": "@good_netlify_hook",
        "GOOD_INTERNET_AIRTABLE_API_KEY": "@good_airtable_key",
        "GOOD_INTERNET_BASE_ID": "@good_airtable_base"
    }
}
